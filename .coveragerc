[run]
source = .
omit = 
    */venv/*
    */env/*
    */migrations/*
    */settings/*
    */wsgi.py
    */asgi.py
    manage.py
    */tests.py
    */test_*.py
    */conftest.py
    */node_modules/*
    */static/*
    */media/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
